{
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "front/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    }
  ]
}
